<?xml version="1.0"?>

<PropertyList>
	<name>power-output-dialog</name>
	<x>-2</x>
	<y>2</y>
	<layout>hbox</layout>
	<default-padding>2</default-padding>
	<font>
		<name>HELVETICA_30</name>
	</font>
	<color>
		<red>0</red>
		<green>0</green>
		<blue>0</blue>
		<alpha>0</alpha>
	</color>
	<text>
		<label>000</label>
		<property>fdm/jsbsim/human-output/electric-motor</property>
        <format>%3.0f</format>
		<live>true</live>
		<color>
			<red>255</red>
			<green>255</green>
			<blue>50</blue>
			<alpha>1</alpha>
		</color>
	</text>
</PropertyList>
